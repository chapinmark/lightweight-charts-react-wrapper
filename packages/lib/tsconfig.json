{
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "target": "es6",
    "rootDir": "./src",
    "outDir": "./dist",
    "jsx": "react-jsx",
    "importHelpers": true,
    "skipLibCheck": false,
    "isolatedModules": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "moduleResolution": "node",
    "types": [],
  },
  "include": ["src"],
}
